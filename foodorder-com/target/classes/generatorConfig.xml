        <!-- 生成Dao类存放位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.foodorder.userserver.dao" targetProject="F:\file">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>


        <!-- 生成对应表及类名 -->
        <table tableName="order_shop" domainObjectName="OrderShop" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="order_user" domainObjectName="OrderUser" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>



    </context>
</generatorConfiguration>